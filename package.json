{
  "name": "ts-chrome-extension",
  "version": "1.0.0",
  "description": "Chrome Extension starter built with TypeScript and Parcel",
  "scripts": {
    "build:content": "parcel build src/content/index.ts --dist-dir dist/content --no-source-maps",
    "build": "npm run build:content",
    "clean": "rimraf dist",
    "copy:static": "copyfiles manifest.json  images/**/* dist",
    "dist": "npm run clean && npm run build && npm run copy:static"
  },
  "author": "Adeel Shafqat",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "@parcel/transformer-vue": "^2.7.0",
    "@types/chrome": "^0.0.197",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.8.2",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vue/component-compiler-utils": "^3.3.0",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "copyfiles": "^2.4.1",
    "eslint": "^8.24.0",
    "eslint-plugin-react": "^7.31.8",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "parcel": "^2.8.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "url": "^0.11.0",
    "util": "^0.12.5"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/auto-instrumentations-node": "^0.35.0",
    "@opentelemetry/context-zone": "^1.8.0",
    "@opentelemetry/exporter-collector": "^0.25.0",
    "@opentelemetry/exporter-jaeger": "^1.8.0",
