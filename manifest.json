{
	"manifest_version": 3,
	"name": "Metrics Collector",
	"version": "1.0.0",
	"description": "",
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"content_scripts": [{
		"js": [
			"content/index.js"
		],
		"matches": ["<all_urls>"]
	}],
	"permissions": [
		"tabs"
	],
	"host_permissions": [
		"*://*/*"
	]
}
